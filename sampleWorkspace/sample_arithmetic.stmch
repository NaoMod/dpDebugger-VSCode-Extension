StateMachine SampleMachine {
    INITIAL -> A;

    state A {
        -> B ['b' /
            y=1;
        ];
        -> B ['b' / 
            y=12;
            a=0;
        ];
    }

    composite state B {
        -> A ['a' /
            a=y+5;
            b=2;
            b=3;
            b=2;
        ];
        -> FINAL ['final'];

        INITIAL -> C;

        state C {
            -> D ['d'];
        }

        state D {
            -> C ['c'];
        }
    }
}